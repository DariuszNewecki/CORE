# Database
DATABASE__URL=postgresql+asyncpg://postgres:postgres@localhost:5432/core
DATABASE__ECHO=false
DATABASE__POOL_SIZE=5
DATABASE__MAX_OVERFLOW=10

# Secrets Management
SECRETS__MASTER_KEY=your-base64-encoded-32-byte-key-here
SECRETS__ROTATION_DAYS=90

# Qdrant
QDRANT__URL=http://localhost:6333
QDRANT__COLLECTION_NAME=core_capabilities
QDRANT__VECTOR_SIZE=384

# LLM
LLM__PROVIDER=anthropic
LLM__MODEL=claude-sonnet-4-5-20250929
LLM__MAX_TOKENS=4096

# Logging
LOG__LEVEL=INFO

# Environment
ENVIRONMENT=development
